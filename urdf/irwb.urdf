<?xml version="1.0"?>
<!-- http://wiki.ros.org/urdf/XML/link -->
<robot name="runway_bot">

  <!--### Body of iRWB ###-->
  <link name="body_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="2 1 0.7"/>
      </geometry>
      
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
  </link>
  
   <!-- Joint from iRWB to wheel 1 -->
 <joint name="wheel1_joint" type="continuous">
  <parent link="body_link"/>
  <child link="wheel1_link"/>
  <origin rpy="0 0 0" xyz="-0.6875 -0.6 -0.300"/>
  <axis xyz="0 1 0"/>
 </joint>
  
  <!--### back right wheel1 ###-->
  <link name="wheel1_link">
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.15" radius="0.1875"/>
      </geometry>
   
      <material name="yellow">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>
  
   <!-- Joint from iRWB to wheel 2 -->
 <joint name="wheel2_joint" type="continuous">
  <parent link="body_link"/>
  <child link="wheel2_link"/>
  <origin rpy="0 0 0" xyz="-0.6875 0.6 -0.300"/>
  <axis xyz="0 1 0"/>
 </joint>
  
  <!--### back left wheel2 ###-->
  <link name="wheel2_link">
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.15" radius="0.1875"/>
      </geometry>
   
      <material name="yellow">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>
  
   <!-- Joint from iRWB to wheel 3 -->
 <joint name="wheel3_joint" type="continuous">
  <parent link="body_link"/>
  <child link="wheel3_link"/>
  <origin rpy="0 0 0" xyz="0.6875 -0.6 -0.300"/>
  <axis xyz="0 1 0"/>
 </joint>
  
  <!--### front right wheel3 ###-->
  <link name="wheel3_link">
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.15" radius="0.1875"/>
      </geometry>
   
      <material name="yellow">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>
  
   <!-- Joint from iRWB to wheel 4 -->
 <joint name="wheel4_joint" type="continuous">
  <parent link="body_link"/>
  <child link="wheel4_link"/>
  <origin rpy="0 0 0" xyz="0.6875 0.6 -0.300"/>
  <axis xyz="0 1 0"/>
 </joint>
  
  <!--### front left wheel4 ###-->
  <link name="wheel4_link">
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.15" radius="0.1875"/>
      </geometry>
   
      <material name="yellow">
        <color rgba="1 1 0 1"/>
      </material>
    </visual>
  </link>
 
</robot>
